{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Agnitra",
  "favicon": "/favicon.svg",
  "github": "https://github.com/agnitraai/agnitraai",
  "colors": {
    "primary": "#ff4f5a",
    "light": "#f7f7f8",
    "dark": "#1f1f24"
  },
  "navigation": [
    {
      "group": "Overview",
      "pages": ["docs", "publishing"]
    },
    {
      "group": "Guides",
      "pages": ["cli-guide"]
    },
    {
      "group": "Maintainers",
      "pages": ["docs-deployment"]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/docs",
      "permanent": true
    },
    {
      "source": "/getting-started",
      "destination": "/docs/getting-started",
      "permanent": true
    }
  ],
  "ignore": ["deploy/**", "internal-docs/**"]
}
