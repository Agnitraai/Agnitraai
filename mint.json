{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Agnitra",
  "favicon": "/favicon.svg",
  "github": "https://github.com/agnitraai/agnitraai",
  "colors": {
    "primary": "#ff4f5a",
    "light": "#f7f7f8",
    "dark": "#1f1f24"
  },
  "navigation": [
    {
      "group": "Overview",
      "pages": ["docs", "quickstart"]
    },
    {
      "group": "Build",
      "pages": ["python-sdk", "javascript-sdk", "cli"]
    },
    {
      "group": "Operate",
      "pages": ["telemetry", "operations"]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/docs",
      "permanent": true
    }
  ],
  "ignore": ["deploy/**", "internal-docs/**"]
}
