{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Agnitra",
  "favicon": "/favicon.svg",
  "github": "https://github.com/agnitraai/agnitraai",
  "colors": {
    "primary": "#ff4f5a",
    "light": "#f7f7f8",
    "dark": "#1f1f24"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Start Here",
            "pages": [
              "index",
              "intro/quickstart",
              "intro/architecture"
            ]
          },
          {
            "group": "Product Guides",
            "pages": [
              "guides/cli-and-sdk",
              "guides/telemetry",
              "guides/marketplace"
            ]
          }
        ]
      },
      {
        "tab": "Reference",
        "groups": [
          {
            "group": "Platform",
            "pages": [
              "reference/configuration",
              "reference/control-plane",
              "reference/responses-api"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Open Console",
          "href": "https://console.agnitra.ai",
          "icon": "rocket"
        },
        {
          "anchor": "Slack Community",
          "href": "https://join.slack.com/t/agnitra/shared_invite/abcdef",
          "icon": "slack"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "CLI Reference",
        "href": "https://github.com/agnitraai/agnitraai/tree/main/cli"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://console.agnitra.ai"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/agnitraai/agnitraai",
      "x": "https://x.com/agnitraai",
      "linkedin": "https://www.linkedin.com/company/agnitra-ai/"
    }
  },
  "redirects": [],
  "ignore": ["deploy/**", "internal-docs/**"]
}
